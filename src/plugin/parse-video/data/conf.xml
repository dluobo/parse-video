       </crawl>  
       <crawl type="videoplay" urlpattern="ddddd" extendclass="org.apache.nutch.parse.html.video.youku.YoukuVideoInfoFilter">
	 <field name="vid" xpath=""/>
	 <field name="name" xpath=""/>
	 <field name="year" xpath=""/>
	 <field name="scole" xpath=""/>
       </crawl>  
  </web>
</conf>